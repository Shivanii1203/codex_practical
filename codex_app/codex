     List<String>Javacodes =['''import 'dart:io';
class parent{
int x = 10;
String str ='Shivani';
void parentdata(){
print(x);
print(this.hashCode);
  }
}
class child extends parent{
int x = 20;
void childdata(){
print(x);
print(this.hashCode);
 }
}
void main(){
child c = new child();
c.childdata();
c.parentdata();
}''','''import 'dart:io';
abstract class parent{
 void education();
 void marry(){
 print('Shivani');
  }
}

class child extends parent{
 void education(){
 print('Btech');
 }
}

void main(){
 child c = new child();
 c.education();
 c.marry();
}'''];

