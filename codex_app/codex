     Widget reusableContainer(BuildContext context,String name,String imgurl,List topiclist,List codelist){
    return InkWell(
      onTap: () {
        
        Navigator.push(context, MaterialPageRoute(builder: (context)=>topics(name:name,topic:topiclist,code:codelist)),);
      },
      child: Container(
        
                height: 250,
                width: 350,
                            
                            margin: EdgeInsets.only(top:20,left: 50,bottom: 10,right: 20),
                            decoration: BoxDecoration(
                              image: DecorationImage(image: NetworkImage(imgurl),fit: BoxFit.fill),
                              color: Colors.white,
                              boxShadow:[
                                BoxShadow(
                                  color:  Colors.black,
                                  offset: Offset(10, 10),
                                  blurRadius: 10,
                                ),
                                ]
                            ),
                          
                        ),
                        
    );
                     
  
   }

